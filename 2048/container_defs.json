[
  {
    "name": "${app_name}",
    "image": "alexwhen/docker-2048",
    "cpu": 0,
    "memory": null,
    "memoryReservation": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 8080
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${environment}-2048",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "web"
      }
    }
  }
]
